<!DOCTYPE html>
<html>
  <head>
    <title>Try</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% flash.each do |key,value| %>
    <h5 style="color : green"><%= value %> </h5>
    <% end %>

    <%= yield %>
  </body>
</html>
